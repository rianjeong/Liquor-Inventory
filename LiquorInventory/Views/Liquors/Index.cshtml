@model IEnumerable<LiquorInventory.Models.Liquor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.liquorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.liquorCost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.liquorInventory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.liquorInventoryCost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.liquorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.liquorCost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.liquorInventory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.liquorInventoryCost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.liquorId }) |
            @Html.ActionLink("Details", "Details", new { id=item.liquorId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.liquorId })
        </td>
    </tr>
}

</table>
